<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>ServletAniadirCategoria</servlet-name>
        <servlet-class>controladorEditarLibro.ServletAniadirCategoria</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ForgotPassword</servlet-name>
        <servlet-class>controlador.ForgotPassword</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ValidarClave</servlet-name>
        <servlet-class>controlador.ValidarClave</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NuevaContrasenia</servlet-name>
        <servlet-class>controlador.NuevaContrasenia</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ChangePassword</servlet-name>
        <servlet-class>controlador.ChangePassword</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Pdf</servlet-name>
        <servlet-class>controladorEditarLibro.Pdf</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletAniadirCategoria</servlet-name>
        <url-pattern>/ServletAniadirCategoria</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ForgotPassword</servlet-name>
        <url-pattern>/ForgotPassword</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ValidarClave</servlet-name>
        <url-pattern>/ValidarClave</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NuevaContrasenia</servlet-name>
        <url-pattern>/NuevaContrasenia</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ChangePassword</servlet-name>
        <url-pattern>/ChangePassword</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Pdf</servlet-name>
        <url-pattern>/Pdf</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
